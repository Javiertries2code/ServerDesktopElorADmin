<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
<type-mapping>
	<sql-type jdbc-type="LONGVARBINARY" hibernate-type="binary"
		not-null="false">
	</sql-type>
	<sql-type jdbc-type="BIT" hibernate-type="java.lang.Boolean"
		not-null="false">
	</sql-type>
	<sql-type jdbc-type="DATE" hibernate-type="date" length="10"
		not-null="false">
	</sql-type>
	<sql-type jdbc-type="INTEGER" hibernate-type="int"
		not-null="true">
	</sql-type>
	<sql-type jdbc-type="CHAR" hibernate-type="string" length="6"
		not-null="false">
	</sql-type>
	<sql-type jdbc-type="VARCHAR" hibernate-type="string" length="180"
		not-null="false">
	</sql-type>
	<sql-type jdbc-type="TIMESTAMP" hibernate-type="timestamp"
		length="19" not-null="false">
	</sql-type>
</type-mapping>
<table-filter match-schema="ElorBase" match-name=".*"></table-filter>
	<table-filter match-name="course" match-catalog="ElorBase"></table-filter>
	<table-filter match-name="meeting" match-catalog="ElorBase"></table-filter>
	<table-filter match-name="meeting_request"
		match-catalog="ElorBase">
	</table-filter>
	<table-filter match-name="registration" match-catalog="ElorBase"></table-filter>
	<table-filter match-name="schedule" match-catalog="ElorBase"></table-filter>
	<table-filter match-name="student" match-catalog="ElorBase"></table-filter>
	<table-filter match-name="subject" match-catalog="ElorBase"></table-filter>
	<table-filter match-name="subject_course"
		match-catalog="ElorBase">
	</table-filter>
	<table-filter match-name="teacher" match-catalog="ElorBase"></table-filter>
</hibernate-reverse-engineering>